doi = {10.1016/j.dsr2.2014.11.003},
file = {:C$\backslash$:/Users/Junju/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Rigaud et al. - 2015 - sup210supPo and sup210supPb distribution, dissolved-particulate exchange rates, and particulate export along the.pdf:pdf},
