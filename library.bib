@article{Ciceri1992,
abstract = {Concentrations of the heavy metals Cd, Co, Cr, Cu, Fe, Mn, Ni, Pb and Zn were measured in sea water, suspended matter, sediments and pore water samples collected in a coastal area of the middle Tyrrhenian Sea. Concentration factors between pore water (extracted from the first centimeter of the sediments) and the overlying sea water (taken 30 cm above the sea bed) were less than 1 for Cr, Cu and Pb, 1-10 for Cd and Ni, 10-100 for Fe and Co, 100-1000 for Mn, and 1-100 for Zn. The benthic fluxes of heavy metals at the sediment-water interface were measured directly using in situ benthic chambers and calculated using Fick's first law during two experimental periods, one in 1986 and the other in 1988. The fluxes of Cu, Ni, Pb and Zn varied significantly over time; this appeared to be related to their relatively low (less-than-or-equal-to 10) concentration factors. From the benthic chamber experiments, metals with positive fluxes were in the order: Mn {\textgreater} Fe {\textgreater} Co {\textgreater} Cd, while those with negative fluxes were: Zn {\textgreater} Pb {\textgreater} Ni congruent-to Cu. Fluxes calculated using Fick's Law were: positive - Mn {\textgreater} Fe {\textgreater} Zn (or Zn {\textgreater} Fe) {\textgreater} Ni {\textgreater} Co {\textgreater} Cd, negative fluxes Pb {\textgreater} Cu {\textgreater} Cr. Measured (benthic chamber) and calculated (Fick's first law) fluxes for Co, Cd, Mn, Pb and Fe were comparable within an order of magnitude, although less agreement was found for Cu, Ni and Zn. Removal of Ni and Zn at the sediment-water interface has been proposed to explain the fact that the measured and calculated fluxes have opposite directions for these metals.},
author = {Ciceri, G and Maran, S and Martinotti, W and Queirazza, G},
file = {:C$\backslash$:/Users/Junju/Desktop/Imperial Y2/Independent Earth Science Project/Pb literature/Geochemical cycling of heavy metals in a marine coastal area benthic flux determination from pore water profiles and in situ measurements using benthic chambers.pdf:pdf},
