issn = {0161-5149},
journal = {Metal ions in biological systems},
keywords = {Environmental Exposure,Environmental Pollution,Humans,Industrial Waste,Lead,Lead: analysis,Lead: chemistry,Lead: metabolism,Lead: toxicity,Plants,Plants: metabolism,Time},
