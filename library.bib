@article{Burckel2015,
abstract = {During the last glacial period, Greenland's climate shifted between cold (stadial) and warm (interstadial) phases that were accompanied by ocean circulation changes characterized by reduced Atlantic Meridional Overturning Circulation (AMOC) during stadials. Here we present new data from the western tropical Atlantic demonstrating that AMOC slowdowns preceded some of the large South American rainfall events that took place during stadials. Based on 231 Pa/ 230 Th and Ti/Ca measurements in the same sediment core, we determine that the AMOC started to slowdown 1420 ± 250 and 690 ± 180 (1$\sigma$) years before the onset of two large precipitation events associated with Heinrich stadials. Our results bring unprecedented evidence that AMOC changes could be at the origin of the large precipitation events observed in tropical South America during Heinrich stadials. In addition, we propose a mechanism explaining the differences in the extent and timing of AMOC slowdowns associated with shorter and longer stadials.},
author = {Burckel, Pierre and Waelbroeck, Claire and Gherardi, Jeanne-M. and Pichat, Sylvain and Arz, Helge and Lippold, Joerg and Dokken, Trond and Thil, Fran{\c{c}}ois},
doi = {10.1002/2014GL062512.Received},
file = {:C$\backslash$:/Users/Junju/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Burckel et al. - 2015 - Atlantic Ocean circulation changes preceded millenial tropical South America rainfall events during the last gla.pdf:pdf},
issn = {00948276},
journal = {Geophysical Research Letters},
keywords = {10.1002/2014GL062512 and Heinrich stadials,AMOC,Dansgaard-Oeschger stadials,ITCZ,Pa/Th},
pages = {1--8},
title = {{Atlantic Ocean circulation changes preceded millenial tropical South America rainfall events during the last glacial}},
year = {2015}
}
