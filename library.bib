file = {:C$\backslash$:/Users/Junju/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Vallelonga et al. - 2002 - The lead pollution history of Law Dome, Antarctica, from isotopic measurements on ice cores 1500 AD to 1989 A.pdf:pdf},
isbn = {0012-821X},
issn = {0012821X},
journal = {Earth and Planetary Science Letters},
keywords = {Antarctica,Law Dome,barium,ice,lead isotopes,pollution},
number = {1-2},
pages = {291--306},
title = {{The lead pollution history of Law Dome, Antarctica, from isotopic measurements on ice cores: 1500 AD to 1989 AD}},
volume = {204},
