journal = {Marine Chemistry},
keywords = {Aerosols,Atlantic Ocean,Atmospheric deposition,GEOTRACES,Solubility,Trace metals},
pages = {1--12},
publisher = {The Authors},
title = {{Atmospheric trace metal concentrations, solubility and deposition fluxes in remote marine air over the south-east Atlantic}},
url = {http://dx.doi.org/10.1016/j.marchem.2015.06.028},
volume = {177},
