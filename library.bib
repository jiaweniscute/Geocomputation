issn = {00264598},
journal = {Mineralium Deposita},
number = {3},
pages = {185--193},
title = {{Ore-lead isotope pattern from the Iglesiente-Sulcis Area (SW Sardinia) and the problem of remobilization of metals}},
volume = {20},
