title = {{Heavy Metals Toxicity and the Environment}},
url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC4144270/ http://www.scopus.com/inward/record.url?eid=2-s2.0-77950880787{\&}partnerID=tZOtx3y1},
