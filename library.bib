journal = {Hydrobiologia},
keywords = {benthic chamber,benthic fluxes,heavy metals,pore water},
pages = {501--517},
title = {{Geochemical Cycling of Heavy-Metals in a Marine Coastal Area - Benthic Flux Determination From Pore Water Profiles and Insitu Measurements Using Benthic Chambers}},
volume = {235},
year = {1992}
}
