isbn = {00401951},
issn = {00401951},
journal = {Tectonophysics},
keywords = {Earthquake triggering,Ocean-bottom pressure,Slow slip event,Subduction zone,The 2011 Tohoku-Oki earthquake,Volumetric strain},
pages = {14--26},
