journal = {Science of the Total Environment},
keywords = {Benthic fluxes,Metals,Nitrogen,Po River outflow},
number = {2-3},
pages = {121--137},
pmid = {10696718},
title = {{Benthic fluxes of cadmium, lead, copper and nitrogen species in the northern Adriatic Sea in front of the River Po outflow, Italy}},
