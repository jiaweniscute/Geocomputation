doi = {10.1029/1999GL900287},
file = {:C$\backslash$:/Users/Junju/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Alleman et al. - 1999 - Invasion of the abyssal North Atlantic by modern anthropogenic lead.pdf:pdf},
isbn = {0094-8276},
issn = {00948276},
journal = {Geophysical Research Letters},
keywords = {doi:10.1029/1999GL900287,http://dx.doi.org/10.1029/1999GL900287},
number = {15},
pages = {1477--1480},
title = {{Invasion of the abyssal North Atlantic by modern anthropogenic lead}},
