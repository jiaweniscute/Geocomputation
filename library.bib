journal = {Resource Geology},
keywords = {Brine,Lithium,Salar de Atacama,Salar de Uyuni},
number = {2},
pages = {91--101},
title = {{Effects of the chemical compositions of Salars de Uyuni and Atacama brines on lithium concentration during evaporation}},
volume = {64},
